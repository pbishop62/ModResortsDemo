           IF KEYWORD-COUNT < URIMAP-KEYWORD-COUNT OR
              DEF-PADDING OF URIMAP-DEF NOT EQUAL TO SPACES THEN
                MOVE KEYWORD-TABLE-ERROR TO RETURN-CODE
           ELSE
                MOVE 'URIM' TO RESOURCE-TYPE-F
                MOVE  FORMATTED-AREA TO URIMAP-F
                WRITE URIMAP-F.

