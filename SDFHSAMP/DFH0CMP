         DFHMDF POS=(5,33),LENGTH=2,ATTRB=(PROT,BRT),INITIAL='NE'
         DFHMDF POS=(5,36),LENGTH=9,ATTRB=(PROT,NORM),JUSTIFY=LEFT,    *
               INITIAL='- NEW'
