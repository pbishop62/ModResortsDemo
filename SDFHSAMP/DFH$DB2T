CREATE TABLE ATOMSERVICE
   (ATOMSERVICE   CHAR(8),
    RDOGROUP      CHAR(8),
    DESCRIPTION   CHAR(58),
    ATOMTYPE      CHAR(10),
    STATUS        CHAR(8),
    CONFIGFILE    CHAR(255),
    RESOURCENAME  CHAR(16),
    RESOURCETYPE  CHAR(7),
    BINDFILE      CHAR(255),
    DEFINETIME    CHAR(17),
    CHANGETIME    CHAR(17),
    CHANGEUSRID   CHAR(8),
    CHANGEAGENT   CHAR(8),
    CHANGEAGREL   CHAR(4))
   IN <data base name>.<table space>;
