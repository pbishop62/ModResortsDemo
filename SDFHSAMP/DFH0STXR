           03  FILLER                  PIC X(20)
               VALUE '% of Limit. Limit = '.
           03  ZC-W-002-SECONDARY-SESSIONS   PIC ZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.
           03  FILLER                  PIC X(3)   VALUE SPACES.

       01  ZC-W-EVENT-003.
           03  FILLER                  PIC X(28)
               VALUE 'Peak losers for connection '''.
           03  ZC-W-003-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', modename '''.
           03  ZC-W-003-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-W-003-PEAK-SESSIONS  PIC ZZ9.
           03  FILLER                  PIC X(5)   VALUE '% of '.
           03  ZC-W-003-LOSER-SESSIONS PIC ZZ,ZZ9.
           03  FILLER                  PIC X(7)   VALUE ' Limit.'.
           03  FILLER                  PIC X(4)   VALUE SPACES.

       01  ZC-W-EVENT-004.
           03  FILLER                  PIC X(29)
               VALUE 'Peak winners for connection '''.
           03  ZC-W-004-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', modename '''.
           03  ZC-W-004-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
