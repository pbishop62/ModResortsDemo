           03  ZC-E-009-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', Modename '''.
           03  ZC-E-009-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-E-009-ALLOCATES-FAILED   PIC ZZ,ZZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.
           03  FILLER                  PIC X(4)    VALUE SPACES.

       01  ZC-E-EVENT-010.
           03  FILLER                  PIC X(39)
                   VALUE 'Allocates Failed Other for Connection '''.
           03  ZC-E-010-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', Modename '''.
           03  ZC-E-010-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-E-010-ALLOCATES-FAILED   PIC ZZ,ZZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.
           03  FILLER                  PIC XXX     VALUE SPACES.

       01  ZC-E-EVENT-011.
           03  FILLER                  PIC X(43)
               VALUE 'Allocates Purged by XZIQUE for Connection '''.
           03  ZC-E-011-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', Modename '''.
           03  ZC-E-011-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-E-011-ALLOCATES-PURGED   PIC ZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.
           03  FILLER                  PIC XXX     VALUE SPACES.

       01  ZC-E-EVENT-012.
           03  FILLER                  PIC X(33)
                       VALUE 'Queued Allocates for Connection '''.
           03  ZC-E-012-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(13)
                                           VALUE ''', Modename '''.
           03  ZC-E-012-MODE-NAME      PIC X(8).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-E-012-QUEUED-ALLOCATES   PIC ZZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.
           03  FILLER                  PIC X(13)   VALUE SPACES.

       01  ZC-W-EVENT-001.
           03  FILLER                  PIC X(44)
               VALUE 'Peak inuse receive sessions for connection '''.
           03  ZC-W-001-CONNECTION-NAME    PIC X(4).
           03  FILLER                  PIC X(4)    VALUE ''' = '.
           03  ZC-W-001-PEAK-SESSIONS  PIC ZZ9.
           03  FILLER                  PIC X(20)
               VALUE '% of Limit. Limit = '.
           03  ZC-W-001-PRIMARY-SESSIONS   PIC ZZ,ZZ9.
           03  FILLER                  PIC X       VALUE '.'.

       01  ZC-W-EVENT-002.
           03  FILLER                  PIC X(41)
               VALUE 'Peak inuse send sessions for connection '''.
           03  ZC-W-002-CONNECTION-NAME    PIC X(4).
